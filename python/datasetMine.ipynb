    "image = cv2.imread(\"dataset/training/LIVINGROOM/1.jpg\", 1)\n",
    "mask = cv2.imread(\"dataset/training/LIVINGROOM/1_mask.jpg\", 1)\n",
    "\n",
    "masked = cv2.bitwise_and(image, image, mask=mask)\n",
    "\n",
    "cv2.imshow('mask',masked)\n",
    "cv2.waitKey(0)"
